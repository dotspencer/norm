<nav>
  <div class="logo"><a href="/">Review <span class="red">Norm</span></a></div>
  <ul>
    <% for (var i = 0; i < navData.length; i++) { %>
      <% var d = navData[i];  %>
      <% var status = d.link == req._parsedOriginalUrl.pathname ? "current" : "" %>
      <li><a href="<%= d.link %>" class="<%= status %>" ><%= d.title %></a></li>
    <% } %>
  </ul>
</nav>
